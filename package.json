    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "cross-fetch": "^2.2.2",
    "env-cmd": "^9.0.3",
    "graphql": "^14.4.2",
    "graphql-cli": "^3.0.12",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.3",
    "prisma-binding": "^2.3.15"
  },
